    "highlight.js": "^11.11.1",
    "js-beautify": "^1.15.4",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.12",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "sql-formatter": "^15.6.4",
